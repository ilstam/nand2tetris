/**
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:

    Not(in=sel, out=Notsel);

    And(a=a, b=Notsel, out=NotselAnda);
    And(a=b, b=sel, out=selAndb);

    Or(a=NotselAnda, b=selAndb, out=out);
}
