 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:

    Nand(a=a, b=true, out=aNand1);
    Nand(a=b, b=true, out=bNand1);
    Nand(a=aNand1, b=bNand1, out=out);
}
